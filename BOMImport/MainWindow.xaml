<Window x:Class="BOMImport.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BOMImport"
        mc:Ignorable="d"
        Title="BOMImport - Focused Test Inc." Height="600" Width="800" ResizeMode="NoResize">
    <DockPanel Margin="10" HorizontalAlignment="Stretch">
        <WrapPanel DockPanel.Dock="Top" Margin="0,0,0,10">
            <Button Name="btnOpenFile" Click="BtnOpenFile_Click">Open BOM...</Button>
            <TextBlock Width="297"></TextBlock>
            <TextBlock Text="ERPNext User:   " TextWrapping="Wrap" FontWeight="Bold"/>
            <TextBlock x:Name="usernameTxt" Text="Logging in...    " TextWrapping="Wrap"/>
            <Button Name="btnAPIKey" Click="BtnAPIKey_Click">Credentials</Button>
        </WrapPanel>
        <TextBox Name="txtEditor" />
    </DockPanel>
</Window>
